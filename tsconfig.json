{
  "compilerOptions": {
    "target": "ES2015",
    "module": "CommonJS",
    "declaration": false,
    "downlevelIteration": true,
    "strict": true,
    "noImplicitAny": true,
    "removeComments": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "isolatedModules": true
  },
  "typeRoots": [
    "./node_modules/@types"
  ],
  "types": [
    "node"
  ]
}